      <div class="social-media-icons">
        <h2 class="yellow-text">ADVISORY BOARD</h2>
        <%= image_tag("plus.svg", :class => "arrow-down", :id => "advisory-arrow-down")%>
        <%= image_tag("minus.svg", :class => "arrow-down", :id => "advisory-arrow-up")%>
